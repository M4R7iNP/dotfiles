[global_config]
  borderless = True
  title_hide_sizetext = True
  title_transmit_fg_color = "#a5a2a2"
  title_transmit_bg_color = "#090300"
  title_inactive_fg_color = "#a5a2a2"
  title_inactive_bg_color = "#5c5855"
  smart_copy = False
[keybindings]
  cycle_next = ""
  cycle_prev = ""
  go_prev = ""
  go_up = ""
  go_down = ""
  go_left = ""
  go_right = ""
  close_term = ""
  copy = ""
  close_window = ""
  group_all = ""
  group_tab = ""
  edit_window_title = ""
  edit_tab_title = ""
  edit_terminal_title = ""
[profiles]
  [[default]]
    background_darkness = 0.82
    background_type = transparent
    cursor_color = "#a5a2a2"
    foreground_color = "#ffffff"
    show_titlebar = False
    scrollbar_position = hidden
    scrollback_lines = 2000
    palette = "#000000:#cc0000:#4e9a06:#c4a000:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    word_chars = "-,./?%&#_"
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
